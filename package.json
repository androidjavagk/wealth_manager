{
  "name": "portfolio-analytics-dashboard",
  "version": "1.0.0",
  "description": "Comprehensive portfolio analytics dashboard with React frontend and Node.js backend",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": [
    "portfolio",
    "analytics",
    "dashboard",
    "react",
    "nodejs"
  ],
  "author": "Portfolio Analytics Team",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
